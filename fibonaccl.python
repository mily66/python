#fibonaccl sequence
def fib(x):
  if x==1:
    return 1
  if x==0:
    return 0
  else:
    return fib(x-1)+fib(x-2)

MILY= int(input("What factoral would you like?"))
print(fib(MILY))

#factorial sequence
def factoral(x):
  if x==0:
    return 1
  else:
    return x* factoral(x-1)

Mily= int(input("What factoral would you like?"))
print(factoral(Mily))
